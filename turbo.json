{
  "baseBranch": "origin/main",
  "globalDotEnv": [".env"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "^dev",
        "@typebot.io/prisma#db:generate",
        "@typebot.io/prisma#db:push"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "@typebot.io/prisma#db:generate"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        "public/__ENV.js"
      ],
      "outputMode": "new-only",
      "dotEnv": [".env"]
    },
    "docs#build": {
      "dependsOn": ["api:generate"],
      "outputs": ["build/**"],
      "outputMode": "new-only"
    },
    "api:generate": {
      "dependsOn": ["bot-engine#build", "@typebot.io/prisma#db:generate"],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:cleanDatabase": {
      "dependsOn": ["@typebot.io/prisma#db:generate"],
      "cache": false
    },
    "checkAndReportChatsUsage": {
      "dependsOn": ["@typebot.io/prisma#db:generate"],
      "cache": false
    }
  }
}
