{
  "name": "@typebot.io/js",
  "version": "0.3.17",
  "description": "Javascript library to display typebots on your website",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./web": {
      "types": "./dist/web.d.ts",
      "default": "./dist/web.js"
    }
  },
  "scripts": {
    "dev": "rollup --watch --config rollup.config.js",
    "build": "rollup --config rollup.config.js"
  },
  "dependencies": {
    "@ai-sdk/ui-utils": "0.0.36",
    "@ark-ui/solid": "3.9.0",
    "@fix-webm-duration/fix": "1.0.1",
    "@stripe/stripe-js": "1.54.1",
    "@typebot.io/blocks-bubbles": "workspace:*",
    "@typebot.io/blocks-inputs": "workspace:*",
    "@typebot.io/blocks-logic": "workspace:*",
    "@typebot.io/bot-engine": "workspace:*",
    "@typebot.io/env": "workspace:*",
    "@typebot.io/lib": "workspace:*",
    "@typebot.io/rich-text": "workspace:*",
    "@typebot.io/settings": "workspace:*",
    "@typebot.io/theme": "workspace:*",
    "clsx": "2.0.0",
    "dompurify": "3.0.6",
    "ky": "1.2.4",
    "marked": "9.0.3",
    "solid-element": "1.7.1",
    "solid-js": "1.7.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.24.7",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "26.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.7",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.0",
    "@typebot.io/tsconfig": "workspace:*",
    "@types/dompurify": "3.0.3",
    "autoprefixer": "10.4.14",
    "babel-preset-solid": "1.8.22",
    "postcss": "8.4.26",
    "rollup": "4.22.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-typescript-paths": "1.5.0",
    "tailwindcss": "3.3.3"
  }
}
