{
  "name": "@typebot.io/prisma",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "studio:start": "dotenv -e ./.env -e ../../.env -- node dist/scripts/studio.js",
    "db:generate": "dotenv -e ./.env -e ../../.env -- node dist/scripts/db-generate.js",
    "db:generate:mysql": "DATABASE_URL=mysql:// dotenv -e ./.env -e ../../.env -- node dist/scripts/db-generate.js",
    "db:push": "dotenv -e ./.env -e ../../.env -- node dist/scripts/db-push.js",
    "migrate:deploy": "dotenv -e ./.env -e ../../.env -- node dist/scripts/migrate-deploy.js",
    "migrate:dev": "otenv -e ./.env -e ../../.env -- prisma migrate dev --create-only --schema postgresql/schema.prisma",
    "db:migrate": "bun migrate:deploy"
  },
  "dependencies": {
    "prisma": "5.12.1",
    "@prisma/client": "5.12.1"
  },
  "devDependencies": {
    "@typebot.io/tsconfig": "workspace:*",
    "dotenv-cli": "7.4.1"
  }
}
